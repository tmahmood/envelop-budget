<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="CategoryRow" parent="AdwActionRow">
        <property name="layout-manager">
            <object class="GtkBinLayout"/>
        </property>
        <property name="activatable_widget">category_id_label</property>
        <child type="prefix">
            <object class="GtkButton" id="category_id_label">
                <property name="margin-top">12</property>
                <property name="margin-bottom">12</property>
                <style>
                    <class name="opaque"/>
                    <class name="circular"/>
                </style>
            </object>
        </child>
        <child type="suffix">
            <object class="GtkLabel" id="allocated_label">
                <property name="label">Assigned</property>
                <property name="halign">GTK_ALIGN_END</property>
            </object>
        </child>
    </template>
</interface>

