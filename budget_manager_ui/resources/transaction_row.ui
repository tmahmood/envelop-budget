<?xml version="1.0" encoding="UTF-8"?>
<interface>
    <template class="TransactionRow" parent="GtkBox">
        <property name="layout-manager">
            <object class="GtkBinLayout"/>
        </property>
        <child>
            <object class="GtkBox" id="child">
                <property name="margin-start">10</property>
                <property name="margin-end">10</property>
                <property name="margin-top">12</property>
                <property name="margin-bottom">12</property>
                <property name="orientation">vertical</property>
                <property name="spacing">6</property>
                <child>
                    <object class="GtkBox">
                        <property name="spacing">12</property>
                        <child>
                            <object class="GtkBox">
                                <property name="margin-start">10</property>
                                <property name="margin-end">10</property>
                                <property name="margin-top">12</property>
                                <property name="margin-bottom">12</property>
                                <property name="spacing">12</property>
                                <property name="orientation">horizontal</property>
                                <child>
                                    <object class="GtkImage" id="transaction_type"/>
                                </child>
                                <child>
                                    <object class="GtkButton" id="transaction_id_btn">
                                        <style>
                                            <class name="circular"/>
                                        </style>
                                    </object>
                                </child>
                            </object>
                        </child>
                        <child>
                            <object class="GtkBox">
                                <property name="orientation">vertical</property>
                                <child>
                                    <object class="GtkLabel" id="payee_label">
                                        <property name="hexpand">True</property>
                                        <property name="xalign">0</property>
                                        <property name="ellipsize">none</property>
                                        <property name="lines">0</property>
                                        <property name="wrap">True</property>
                                        <property name="wrap-mode">word-char</property>
                                        <property name="use-markup">True</property>
                                        <property name="label">Payee</property>
                                        <style>
                                            <class name="subtitle"/>
                                        </style>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkLabel" id="note_label">
                                        <property name="ellipsize">none</property>
                                        <property name="lines">0</property>
                                        <property name="wrap">True</property>
                                        <property name="wrap-mode">word-char</property>
                                        <property name="xalign">0</property>
                                        <property name="use-markup">True</property>
                                        <property name="label">Note</property>
                                        <style>
                                            <class name="title"/>
                                        </style>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkLabel" id="date_created_label">
                                        <property name="xalign">0</property>
                                        <property name="ellipsize">none</property>
                                        <property name="lines">0</property>
                                        <property name="use-markup">True</property>
                                        <property name="label">Date Created</property>
                                        <style>
                                            <class name="subtitle"/>
                                        </style>
                                    </object>
                                </child>
                            </object>
                        </child>
                        <child>
                            <object class="GtkBox">
                                <property name="orientation">vertical</property>
                                <child>
                                    <object class="GtkLabel" id="amount_label">
                                        <property name="label">0000.0000</property>
                                        <property name="yalign">0</property>
                                        <property name="halign">GTK_ALIGN_END</property>
                                        <style>
                                            <class name="caption-heading"/>
                                        </style>
                                    </object>
                                </child>
                                <child>
                                    <object class="GtkLabel" id="category_name_label">
                                        <property name="label">Category Name</property>
                                        <property name="halign">end</property>
                                        <style>
                                            <class name="dim-label"/>
                                        </style>
                                    </object>
                                </child>
                            </object>
                        </child>
                    </object>
                </child>
            </object>
        </child>
    </template>
</interface>

